<tr class="small">
    <td></td>
    <td></td>
    <td><%= link_to part.number, part_path(part) %></td>
    <td><%= part.revision %></td>
    <td><%= part.base_material %></td>
    <td><%= part.finish %></td>
    <% if part.measured_status %>
        <td class="text-success">Measured</td>
    <% else%>
        <td class="text-danger">Not measured</td>
    <% end %>
</tr>
