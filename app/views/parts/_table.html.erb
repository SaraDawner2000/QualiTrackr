<table class="table table-bordered table-striped-columns table-hover">
    <thead>
        <tr>
          <th scope="col">
              Quality project
          </th> 
          <th scope="col">
             Customer
          </th>            
          <th scope="col">
              Part number
          </th>
          <th scope="col">
              Part revision
          </th>
          <th scope="col">
              Base material
          </th>
          <th scope="col">
              Finish
          </th>
          <th scope="col">
              Measured?
          </th>
        </tr>
    </thead>

    <% parts.each do |part| %>
        <tbody>
            <%= render "parts/table_field", part: part %>
        </tbody>
    <% end %>
</table>
