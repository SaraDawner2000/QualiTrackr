<div class="container">
  <div class="d-flex justify-content-between align-items-center my-4">
  <h1 class="display-3 mt-4">Dashboard</h1>
  <div>
    <% if current_user.admin? %>
      <%= link_to "Add new user", new_admin_registration_path, class: "btn btn-success" %>
    <% end %>
    <% if current_user.role in ["quality_manager", "prod_manager"] %>
      <%= link_to "Add new quality project", new_part_path, class: "btn btn-primary" %>
    <% end %>
  </div>
  
  </div>
  <%= render "tasks_summary" %>
  <%= render "dashboard_tab_nav" %>
</div>
